
{
    "builds": [
      {
        "src": "thiruth3/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.8"}
      },
    
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "thiruth3/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  
